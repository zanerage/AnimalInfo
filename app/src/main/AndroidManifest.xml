<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mario_antolovic.animalinfo">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".mammals">
        </activity>
        <activity android:name=".Amphibians">
        </activity>
        <activity android:name=".reptiles">
        </activity>
        <activity android:name=".birds">
        </activity>
        <activity android:name=".fish">
        </activity>
        <activity android:name=".invertebrates">
        </activity>
        <activity android:name=".next_phase">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>